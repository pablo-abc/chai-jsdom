{
  "name": "chai-jsdom",
  "version": "0.1.0",
  "description": "jest-dom style assertions for Chai",
  "main": "dist/cjs/index.cjs",
  "browser": "dist/esm/index.mjs",
  "module": "dist/esm/index.mjs",
  "types": "typings/chai/index.d.ts",
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -cw",
    "test": "uvu -r tsm -r global-jsdom/register tests -i common",
    "test:ci": "nyc -n src pnpm test"
  },
  "author": "Pablo Berganza <pablo@berganza.dev>",
  "license": "MIT",
  "files": [
    "dist",
    "typings/chai"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.1",
    "@testing-library/dom": "^8.11.3",
    "@types/chai": "^4.3.0",
    "@types/css": "^0.0.33",
    "chai": "^4.3.6",
    "global-jsdom": "^8.4.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.0",
    "rollup-plugin-ts": "^2.0.5",
    "tsm": "^2.2.1",
    "typescript": "^4.5.5",
    "uvu": "^0.5.3"
  },
  "peerDependencies": {
    "chai": "^4.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "css": "^3.0.0",
    "dom-accessibility-api": "^0.5.11",
    "redent": "^4.0.0"
  }
}
